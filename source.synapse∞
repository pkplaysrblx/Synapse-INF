-- Scripts:

local function MPBMVAH_fake_script() -- exec.LocalScript 
	local script = Instance.new('LocalScript', exec)

	script.Parent.Parent.exec.MouseButton1Click:Connect(function()
		loadstring(script.Parent.Parent.TextBox)()
	end)
end
coroutine.wrap(MPBMVAH_fake_script)()
local function NKVF_fake_script() -- rpos.LocalScript 
	local script = Instance.new('LocalScript', rpos)

	script.Parent.Parent.rpos.MouseButton1Click:Connect(function()
		game.Players.LocalPlayer.PlayerGui.ScreenGui.Panel.Position = UDim2.new(0.053, 0, 0.103, 0)
	end)
end
coroutine.wrap(NKVF_fake_script)()
local function ZRNGV_fake_script() -- clr.LocalScript 
	local script = Instance.new('LocalScript', clr)

	script.Parent.Parent.clr.MouseButton1Click:Connect(function()
		script.Parent.Parent.TextBox.Text = ""
	end)
end
coroutine.wrap(ZRNGV_fake_script)()
local function LCJV_fake_script() -- TextBox.LocalScript 
	local script = Instance.new('LocalScript', TextBox)

	local highlighter = loadstring(game:HttpGet("https://raw.githubusercontent.com/pkplaysrblx/Synapse-INF/refs/heads/main/syntax.synapse∞"))() -- Ensure the module is correctly required
	local textbox = script.Parent.Parent.TextBox -- The TextBox where the user types
	local highlightingLabel = script.Parent.Parent.HighlightLabel -- The TextLabel that will show the highlighted result
	
	-- Enable RichText for TextLabel to allow color formatting
	highlightingLabel.RichText = true
	
	-- Debounce settings
	local debounceTime = 0 -- Time to wait before updating (in seconds)
	local lastInputTime = 0
	
	-- Make the TextBox text invisible (fully transparent)
	textbox.TextTransparency = 1
	textbox.TextStrokeTransparency = 0.5 -- Add some stroke to make text more defined
	textbox.TextSize = 18 -- Set a fixed text size for clearer text (optional)
	textbox.TextScaled = true -- Keep TextScaled enabled for mobile responsiveness
	
	-- Function to update the highlighted text in the TextLabel
	local function updateHighlightedText()
		local sourceText = textbox.Text
		local highlightedText = highlighter.run(sourceText) -- Apply the highlighter module
	
		-- Set the highlighted text to the TextLabel (rich text)
		highlightingLabel.Text = highlightedText
		print(textbox.Text)
	end
	
	-- Function to update text after a delay (debounced)
	local function onTextChanged()
		local currentTime = tick()  -- Current time in seconds
	
		-- Check if enough time has passed since the last update
		if currentTime - lastInputTime >= debounceTime then
			-- Update the highlighted text in the label
			updateHighlightedText()
	
			-- Record the time of the last input update
			lastInputTime = currentTime
		end
	end
	
	-- Run the update function when the text in the TextBox changes
	textbox:GetPropertyChangedSignal("Text"):Connect(onTextChanged)
	
	
	
	
end
coroutine.wrap(LCJV_fake_script)()
local function FHLKSYP_fake_script() -- csl.LocalScript 
	local script = Instance.new('LocalScript', csl)

	script.Parent.Parent.csl.MouseButton1Click:Connect(function()
		local v = game:GetService("VirtualInputManager")
		v:SendKeyEvent(true, Enum.KeyCode.F9, false, game)
	end)
end
coroutine.wrap(FHLKSYP_fake_script)()
local function CDHWTOJ_fake_script() -- Panel.LocalScript 
	local script = Instance.new('LocalScript', Panel)

	local UIS = game:GetService('UserInputService')
	local frame = script.Parent
	local dragToggle = nil
	local dragSpeed = 0.25
	local dragStart = nil
	local startPos = nil
	
	local function updateInput(input)
		local delta = input.Position - dragStart
		local position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X,
			startPos.Y.Scale, startPos.Y.Offset + delta.Y)
		game:GetService('TweenService'):Create(frame, TweenInfo.new(dragSpeed), {Position = position}):Play()
	end
	
	frame.InputBegan:Connect(function(input)
		if (input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch) then 
			dragToggle = true
			dragStart = input.Position
			startPos = frame.Position
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragToggle = false
				end
			end)
		end
	end)
	
	UIS.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			if dragToggle then
				updateInput(input)
			end
		end
	end)
end
coroutine.wrap(CDHWTOJ_fake_script)()
